class Solution:
    def repeatedCharacter(self, s: str) -> str:
        dic = {} # create dictionary 
        for i in range(len(s)): # loop through all characters in string
            letter = s[i] # temporarily assign var letter to current letter in string
            if letter in dic: # if this is in the dictionary [O(1) lookup]
                return letter # return that letter

            dic[letter] = i # or else, just update the hash table with letter:i
    

# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # 

class Solution:
    def repeatedCharacter(self, s: str) -> str:
        seen = set()
        for c in s:
            if c in seen:
                return c
            seen.add(c)

        return " "
