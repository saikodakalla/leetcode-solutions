class Solution {
public:
    int majorityElement(vector<int>& nums) {
        std::unordered_map<int, int> count; 
        int length = nums.size();

        for (int i = 0; i < nums.size(); ++i) {
            count[nums[i]]++;
        }

        for (const auto& [key, value] : count) {
            if (value > (length / 2)) {
                return key;
                }
        }

        return -1;
    }

};
